<% @bookings.each do |booking| %>
  <%= booking.status %>
  <%= booking.start_date %>
  <%= booking.end_date %>
  <%= link_to "Edit", tuxedo_booking_path(booking),
    method: :edit %>
  <%= link_to "Delete", tuxedo_booking_path(booking),
    method: :delete,
    data: { confirm: "Are you sure?" } %>
<% end %>

<%= link_to "Back to home page", tuxedos_path %>

